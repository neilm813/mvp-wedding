{
  "name": "tbd-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "vitest",
    "build:with-tests": "npm run test && next build",
    "prepare": "husky",
    "postinstall": "husky install"
  },
  "dependencies": {
    "next": "15.2.4",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.2.4",
    "husky": "^9.1.7",
    "jsdom": "^26.1.0",
    "lint-staged": "^16.0.0",
    "prettier": "^3.5.3",
    "pretty-quick": "^4.1.1",
    "tailwindcss": "^4",
    "typescript": "^5",
    "vitest": "^3.1.2"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "pretty-quick --staged"
    ],
    "*.{json,css,md}": [
      "pretty-quick --staged"
    ]
  }
}
